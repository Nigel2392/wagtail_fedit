{{ define "head" }}
    <title>{{.Title}} (v{{ .Config.Project.Version }}) </title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="https://unpkg.com/htmx.org@1.9.12" integrity="sha384-ujb1lZYygJmzgSwoxRggbCHcjc0rB2XoQrxeTUQyRjrOnlCoYta87iKBWq3EsdM2" crossorigin="anonymous"></script>
    <style>
        html,
        body {
            margin: 0;
            padding: 0;
            font-family: Arial, sans-serif;
            font-size: 16px;
            line-height: 1.6;
        }
        *, *:before, *:after {
            box-sizing: border-box;
        }
        img {
            max-width: 100%;
            height: auto;
        }
        nav {
            width: 250px;
            position: fixed;
            height: 100%;
            overflow: auto;
            padding: 0;
            margin: 0;
        }
        main {
            margin: 0;
            margin-left: 250px;
            width: calc(100% - 250px);
        }
        main h1, main h2, main h3, main h4, main h5, main h6, main ol, main ul {
            margin: 0;
        }
        h1 {
            font-size: 3em;
        }
        h2 {
            font-size: 2.5em;
        }
        h3 {
            font-size: 1.83em;
        }
        h4 {
            font-size: 1.5em;
        }
        h5 {
            font-size: 1.17em;
        }
        h6 {
            font-size: 1em;
        }
        main > * {
            width: 100%;
        }
        pre {
            background-color: #f4f4f4;
            padding: 8px;
            border-left: 3px solid #ccc;
            margin: 10px 0;
            font-size: 14px;
            overflow: auto;
        }
        *:not(pre, h1, h2, h3, h4, h5, h6) > code {
            font-size: 14px;
            color: #c7254e;
            background-color: #f9f2f4;
            padding: 2px 4px;
            border-radius: 4px;
        }
        *:is(h1, h2, h3, h4, h5, h6):has(> code) {
            color: #a10c31;
            background-color: #f9f2f4;
            padding: 0 8px;
        }
        .navbar {
            display: flex;
            flex-direction: column;
            background-color: #333;
            color: #fff;
        }
        .navbar-logo {
            padding: 10px 20px;
        }
        .navbar-title {
            font-size: 1.5em;
            font-weight: bold;
        }
        .navbar-menu {
            display: flex;
            flex-direction: column;
            list-style-type: none;
            padding: 0;
            margin: 0;
        }
        .navbar-menu li {
            list-style-type: none;
        }
        .navbar-menu li .navbar-item {
            display: block;
            padding: 10px 20px;
            color: #fff;
            text-decoration: none;
        }
        .navbar-menu li a.navbar-item:hover {
            background-color: #555;
        }
        .navbar-menu li .navbar-item-small {
            padding: 5px 20px;
        }
        .navbar-footer {
            margin-top: auto;
            margin-bottom: 10px;
        }
        .main-content-wrapper {
            display: flex;
            flex-wrap: wrap;
            flex-direction: column;
            position: relative;
        }
        .main-content-lint {
            padding: 10px;
            margin-bottom: 10px;
            display: flex;
            flex-wrap: wrap;
            flex-direction: row;
            justify-content: space-between;
            background-color: #f4f4f4;
            position: sticky;
            width: 100%;
            z-index: 1000;
            top: 0;
        }
        .object-information {
            max-width: 250px;
            margin-right: 10px;
            text-wrap: nowrap;
            overflow: hidden;
            white-space: nowrap;
            text-overflow: ellipsis;
        }
        .pagination {
            display: flex;
            flex-direction: row;
            justify-content: space-evenly;
            align-items: center;
            flex: 1;
            gap: 10px;
        }
        .pagination__link {
            text-decoration: none;
            color: #333;
            border-radius: 5px;
        }
        .pagination__link,
        .pagination__placeholder {
            flex: 1;
            text-align: center;
        }
        .main-content {
            padding-top: 10px;
            padding-bottom: 10px;
            padding-left: 20px;
            width: 90%;
        }
        .documentation-links {
            display: flex;
            flex-direction: row;
            gap: 10px;
        }
        .documentation-link {
            text-decoration: none;
        }
    </style>
    <link rel="icon" type="image/png" href="{{ Asset "static/favicon.png" }}">
{{ end }}